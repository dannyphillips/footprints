{
  "name": "footprints",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "dependencies": {
    "change-case": "3.1.0",
    "firebase": "6.1.1",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-router": "5.0.0",
    "react-router-dom": "5.0.0",
    "react-scripts": "3.0.1",
    "semantic-ui-react": "0.83.0",
    "styled-components": "3.4.10"
  },
  "devDependencies": {
    "husky": "2.3.0",
    "jsonlint": "1.6.3",
    "lerna": "3.15.0",
    "lerna-changelog": "0.7.0",
    "lint-staged": "8.1.7",
    "prettier": "1.18.2"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "deploy": "firebase deploy",
    "eject": "react-scripts eject"
  },
  "lint-staged": {
    "*.js": [
      "prettier --trailing-comma es5 --single-quote --write",
      "git add"
    ],
    "*.json": "jsonlint",
    ".*rc": "jsonlint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
